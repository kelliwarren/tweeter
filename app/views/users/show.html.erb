
<div class='row-fluid'>
  <div class='offset1 span4'>
    <div class ='well sidebar-nav'>
    <%content_for(:sidebar) do %>
       <%= form_for(@tweet) do |f| %>
       <%= f.text_field(:content) %>
       <%= f.submit %>
        <%end%>
    <%end%>

    <%= yield(:sidebar)%>
    </div>
  </div>
</div>



<div class="row-fluid">
  <div class="offset6 span4">
    <div class="well sidebar-nav">
    
      <h1>Feed</h1>
      <p><% @tweets.each do |tweet| %></p>
      <div class='tweet-container'>
        <%= tweet.content %>
      </div>
      <%end%>
            
    </div>
  </div><!--/row-->
</div><!--/.well -->
       